<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFrameworks>net9.0;net8.0;net7.0;net6.0</TargetFrameworks>
	  <Version>1.1.2</Version>

	  <Description>Provides the core logic for inter-process communication in the WitRPC framework, enabling .NET applications to spawn external processes and call their services seamlessly as if they were local.</Description>
	  <PackageTags>OutWit;Communication;InterProcess</PackageTags>

	  <RepositoryUrl>https://github.com/dmitrat/WitRPC/tree/main/InterProcess/OutWit.InterProcess</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.3" />
    <PackageReference Include="OutWit.Common" Version="1.2.1" />
    <PackageReference Include="OutWit.Common.CommandLine" Version="1.0.2" />
  </ItemGroup>

</Project>
